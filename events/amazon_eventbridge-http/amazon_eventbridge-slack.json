[
    {
        "id": "20230407_1",
        "processors": [
        {
            "filters": [
            {
                "any": [
                {
                    "exact": {
                    "data.body.detail.eventName": "CreateBucket"
                    }
                },
                {
                    "exact": {
                    "data.body.detail.eventName": "DeleteBucket"
                    }
                }
                ]
            }
            ],
            "transformer": {
            "define": {
                "time": "$.data.body.time",
                "account": "$.data.body.account",
                "eventSource": "$.data.body.detail.eventSource",
                "eventName": "$.data.body.detail.eventName",
                "bucketName": "$.data.body.detail.requestParameters.bucketName"
            },
            "pipeline": [
                {
                "command": [
                    "condition_if",
                    "$.data.desc",
                    "<eventName>",
                    "==",
                    "CreateBucket",
                    "created bucket",
                    "deleted bucket"
                ]
                }
            ],
            "templateJson": {
                "body": {
                "username": "The account **<account>** just **<$.data.desc>** and the name is <bucketName>\n\n<time>."
                }
            }
            }
        }
        ]
    }

]